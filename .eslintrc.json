{
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint",
        "prettier"
    ],
    "root": true,
    "rules": {
        "prettier/prettier" : "warn",
        "@typescript-eslint/no-unused-vars": "warn",
        "@typescript-eslint/no-namespace": "off"
    }
}
